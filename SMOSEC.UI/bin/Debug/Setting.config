<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <serversetting>
    <add key="HttpServerPort" value="80" />
    <add key="TcpServerPort" value="8080" />
    <add key="DeveloperTrace" value="Single" />
    <add key="TracePrint" value="True" />
    <add key="ClientTrace" value="False" />
    <add key="EnableStackView" value="True" />
    <add key="SessionTimeOut" value="1800" />
    <add key="StartupForm" value="" />
    <add key="StartupAssembly" value="" />
    <add key="IsSysLog" value="False" />
    <add key="PurgingTimer" value="60" />
    <add key="MirrorPatternMaxVerifyTimes" value="3" />
    <add key="SecurityKey" value="" />
    <add key="RelativeScreen" value="True" />
    <add key="TextFontScaling" value="True" />
  </serversetting>
  <resources>
    <res name="image" path="Resources\Image" />
    <res name="audio" path="Resources\Audio" />
    <res name="document" path="Resources\Document" />
    <res name="upload" path="Resources\Upload" />
    <res name="media" path="Resources\Media" />
    <res name="web" path="Resources\Web" />
  </resources>
</configuration>